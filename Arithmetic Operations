num1 = int(input("num1: "))
num2 = int(input("num2: "))
add = num1+num2
sub = num1-num2
mul = num1*num2
div = num1/num2
print("Addition of",num1,"and",num2,"=",add)
print("Subtraction of",num1,"and",num2,"=",sub)
print("Multiplication of",num1,"and",num2,"=",mul)
print("Division of",num1,"and",num2,"=",div)
